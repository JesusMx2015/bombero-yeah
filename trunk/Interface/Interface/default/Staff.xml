<?xml version="1.0" encoding="UTF-8" ?>
<XML ID="Option">
	<Schema xmlns="MaietData" xmlns:dt="MaietDataTypes" />
	<FRAME item="Option">
		<FRAMELOOK>NullFrameLook</FRAMELOOK>
		<BOUNDS>
			<X>0</X>
			<Y>0</Y>
			<W>-1</W>
			<H>-1</H>
		</BOUNDS>
		<VISIBLE>false</VISIBLE>
	</FRAME>

	<FRAME item="OptionGroup" parent="Option">
		<FRAMELOOK>DefaultFrameLook</FRAMELOOK>
		<BOUNDS>
			<X>0</X>
			<Y>0</Y>
			<W>400</W>
			<H>200</H>
		</BOUNDS>
		<ALIGN>
			<HALIGN>center</HALIGN>
			<VALIGN>center</VALIGN>
		</ALIGN>
		<VISIBLE>TRUE</VISIBLE>
		<RESIZABLE>false</RESIZABLE>
		<TEXT>Mensaje de bienvenida</TEXT> <!-- 옵션 -->
	</FRAME>

	<LABEL item="Mensaje" parent="OptionGroup">
		<FONT>FONTa9</FONT>
		<TEXTCOLOR>
			<R>205</R>
			<G>205</G>
			<B>205</B>
		</TEXTCOLOR>
		<BOUNDS>
			<X>2</X>
			<Y>30</Y>
			<W>300</W>
			<H>24</H>
		</BOUNDS>
		<TEXT>Bienvenido a LegionGamers GunZ, ¡gracias por entrar!</TEXT> <!-- F2 Key -->
	</LABEL>

	<BUTTON item="CancelOptionButton" parent="OptionGroup">
		<BUTTONLOOK>DefaultButtonLook</BUTTONLOOK>
		<ANCHORS>
			<LEFT>true</LEFT>
			<TOP>true</TOP>
			<RIGHT>true</RIGHT>
			<BOTTOM>true</BOTTOM>
		</ANCHORS>
		<BOUNDS>
			<X>300</X>
			<Y>150</Y>
			<W>100</W>
			<H>32</H>
		</BOUNDS>
		<DEFAULTKEY>ESC</DEFAULTKEY>
		<TEXT>Cerrar</TEXT> <!-- 취소(&amp;C) -->
	</BUTTON>

</XML>
